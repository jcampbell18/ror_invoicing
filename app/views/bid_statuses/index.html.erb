<p id="notice"><%= notice %></p>

<section class="outstanding-invoices">
  <h6>Bids</h6>
  <ul>
    <li>
      <p class="heading">Name</p>
    </li>
    <li>
      <p class="heading">View</p>
    </li>
  </ul>
  <% @bid_statuses.each do |bid_status| %>
    <ul class="ul-lines" key={bid_status.bid_status_id}>
        <li><%= bid_status.name %></li>
        <li><%= link_to 'Edit', edit_bid_status_path(bid_status) %></li>
    </ul>
  <% end %>
</section>

